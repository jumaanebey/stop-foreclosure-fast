{
  "name": "foreclosure-ai-workflows",
  "version": "1.0.0",
  "description": "Google AI-powered workflows for foreclosure assistance",
  "main": "index.js",
  "scripts": {
    "deploy:lead-bot": "gcloud functions deploy qualifyForeclosureLead --runtime nodejs18 --trigger-http --allow-unauthenticated",
    "deploy:doc-processor": "gcloud functions deploy processForeclosureDocument --runtime nodejs18 --trigger-http --allow-unauthenticated",
    "deploy:email": "gcloud functions deploy processForeclosureEmail --runtime nodejs18 --trigger-http --allow-unauthenticated",
    "deploy:dialogflow": "gcloud functions deploy dialogflowWebhook --runtime nodejs18 --trigger-http --allow-unauthenticated",
    "deploy:all": "npm run deploy:lead-bot && npm run deploy:doc-processor && npm run deploy:email && npm run deploy:dialogflow",
    "test": "jest",
    "dev": "nodemon index.js"
  },
  "dependencies": {
    "@google-cloud/dialogflow-cx": "^4.2.0",
    "@google-cloud/documentai": "^7.3.0",
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/vertexai": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "google-auth-library": "^9.6.3",
    "google-spreadsheet": "^4.1.1",
    "googleapis": "^131.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "twilio": "^4.20.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "My Foreclosure Solution",
  "license": "MIT"
}
